<%- include('partials/navbar', { page: '/solutions/projects' }) %>

<h1>Projects</h1>
<ul>
  <% projects.forEach(project => { %>
    <li>
      <h2><%= project.title %></h2>
      <p>Sector: <%= project.Sector.sector_name %></p>
      <a href="/solutions/editProject/<%= project.id %>" class="btn btn-success">Edit</a>
    </li>
  <% }) %>
</ul>
